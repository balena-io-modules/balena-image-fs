{
  "name": "balena-image-fs",
  "version": "7.0.2",
  "description": "Balena.io image filesystem manipulation utilities",
  "main": "build/index.js",
  "homepage": "https://github.com/balena-io-modules/balena-image-fs",
  "repository": {
    "type": "git",
    "url": "git://github.com/balena-io-modules/balena-image-fs.git"
  },
  "keywords": [
    "balena",
    "fs",
    "filesystem",
    "device",
    "image"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "balena-lint --typescript lib tests",
    "lint-fix": "balena-lint --fix --typescript lib tests",
    "clean": "rm -rf build",
    "build": "npm run clean && tsc",
    "test": "npm run lint && mocha -r ts-node/register tests/e2e.ts",
    "readme": "jsdoc2md --template doc/README.hbs build/index.js > README.md",
    "prepublish": "npm run test && npm run build && npm run readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.ts": [
      "npm run lint-fix"
    ]
  },
  "files": [
    "build/*.js",
    "build/*.js.map",
    "build/*.d.ts",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "author": "Juan Cruz Viotti <juan@balena.io>",
  "contributors": [
    "Alexis Svinartchouk <alexis@balena.io>",
    "Juan Cruz Viotti <juan@balena.io>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@balena/lint": "^5.1.0",
    "@types/bluebird": "^3.5.32",
    "@types/mocha": "^8.0.0",
    "@types/node": "^10.17.28",
    "@types/tmp": "^0.2.0",
    "bluebird": "^3.7.2",
    "husky": "^4.2.5",
    "jsdoc-to-markdown": "^5.0.3",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "tmp": "^0.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "ext2fs": "^2.0.0",
    "fatfs": "^0.10.7",
    "file-disk": "^8.0.0",
    "partitioninfo": "^6.0.1",
    "typed-error": "^3.2.0"
  }
}
